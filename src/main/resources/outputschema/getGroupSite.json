{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Output Schema for Get Group Site Operation",
  "description": "Output schema for retrieving group site details.",
  "properties": {
    "id": {
      "type": "string"
    },
    "isPersonalSite": {
      "type": "boolean"
    },
    "displayName": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "createdDateTime": {
      "type": "string",
      "format": "date-time"
    },
    "lastModifiedDateTime": {
      "type": "string",
      "format": "date-time"
    },
    "webUrl": {
      "type": "string",
      "format": "uri"
    }
  },
  "required": [
    "id",
    "isPersonalSite",
    "displayName",
    "name",
    "createdDateTime",
    "lastModifiedDateTime",
    "webUrl"
  ],
  "additionalProperties": false
}
