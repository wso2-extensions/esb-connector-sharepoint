{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Output Schema for Create Group Operation",
  "description": "Output schema for the create group operation in the connector.",
  "properties": {
    "id": {
      "type": "string",
      "description": "The unique identifier of the group."
    },
    "deletedDateTime": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time",
      "description": "The date and time when the group was deleted."
    },
    "classification": {
      "type": [
        "string",
        "null"
      ],
      "description": "The classification of the group."
    },
    "createdDateTime": {
      "type": "string",
      "format": "date-time",
      "description": "The date and time when the group was created."
    },
    "description": {
      "type": "string",
      "description": "The description of the group."
    },
    "displayName": {
      "type": "string",
      "description": "The display name of the group."
    },
    "groupTypes": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "description": "The types of the group."
    },
    "mail": {
      "type": "string",
      "description": "The mail address of the group."
    },
    "mailEnabled": {
      "type": "boolean",
      "description": "Indicates whether the mail is enabled for the group."
    },
    "mailNickname": {
      "type": "string",
      "description": "The mail nickname of the group."
    },
    "onPremisesLastSyncDateTime": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time",
      "description": "The date and time of the last sync."
    },
    "onPremisesSecurityIdentifier": {
      "type": [
        "string",
        "null"
      ],
      "description": "The on-premises security identifier."
    },
    "onPremisesSyncEnabled": {
      "type": [
        "boolean",
        "null"
      ],
      "description": "Indicates whether on-premises sync is enabled."
    },
    "preferredDataLocation": {
      "type": "string",
      "description": "The preferred data location."
    },
    "proxyAddresses": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "description": "The proxy addresses of the group."
    },
    "renewedDateTime": {
      "type": "string",
      "format": "date-time",
      "description": "The date and time when the group was renewed."
    },
    "resourceBehaviorOptions": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "description": "The resource behavior options."
    },
    "resourceProvisioningOptions": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "description": "The resource provisioning options."
    },
    "securityEnabled": {
      "type": "boolean",
      "description": "Indicates whether the group is security enabled."
    },
    "visibility": {
      "type": "string",
      "description": "The visibility setting of the group."
    },
    "onPremisesProvisioningErrors": {
      "type": "array",
      "items": {
        "type": "object"
      },
      "description": "The on-premises provisioning errors."
    }
  },
  "required": [
    "id",
    "createdDateTime",
    "description",
    "displayName",
    "groupTypes",
    "mail",
    "mailEnabled",
    "mailNickname",
    "preferredDataLocation",
    "renewedDateTime",
    "securityEnabled",
    "visibility"
  ],
  "additionalProperties": false
}
